{
    "name": "globalcipta/laravel-sms-media-channel",
    "description": "Sends SMS using SMS Media API",
    "homepage": "https://globalcipta.co.id/",
    "license": "MIT",
    "authors": [
      {
        "name": "Rifki Alhuraibi",
        "email": "veelasky@gmail.com",
        "homepage": "https://veelasky.github.io",
        "role": "Developer"
      }
    ],
    "require": {
        "php": ">=7.2.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/notifications": "5.*|6.*",
        "illuminate/queue": "5.*|6.*",
        "illuminate/support": "5.*|6.*"
    },
    "require-dev": {
      "mockery/mockery": "^0.9.5",
      "phpunit/phpunit": "^8.3"
    },
    "autoload": {
      "psr-4": {
        "NotificationChannels\\SmsMedia\\": "src"
      }
    },
    "extra": {
      "branch-alias": {
        "dev-master": "1.2-dev"
      },
      "laravel" : {
        "providers" : [
          "NotificationChannels\\SmsMedia\\SmsMediaServiceProvider"
        ]
      }
    },
    "config": {
      "sort-packages": true
    }
